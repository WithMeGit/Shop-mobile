
@{
    ViewBag.Title = "PageMain_layout";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<div id="wrapper">
    <h1 class="col-md-12">Manager Mobile</h1>
    <div class="form-group">
        <div class="col-md-6">
            <label>Search Name</label>
            <input type="text" id="SearchMobileName" class="form-control" placeholder="Search ..." />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-6">
            <label>Search Type</label>
            <input type="text" id="SearchType" class="form-control" placeholder="Search ..." />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-12">
            &nbsp;
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-12">
            <button type="button" id="btnSearch" class="btn btn-primary">Search</button>
        </div>
    </div>

    <h1 class="col-md-12">List Mobile </h1>
    <div class="col-md-12">
        <button type="button" id="btnCreateNew" data-toggle="modal" data-target="#CreateEditModal"
                class="btn btn-success">
            Create New
        </button>
    </div>

    @RenderSection("MOBILE", required: false)

</div>

@RenderSection("MODEL_DIALOG", required: false)

@RenderSection("Script", required: false)


